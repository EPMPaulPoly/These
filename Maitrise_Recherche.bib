	note = {Google-Books-ID: 9DqQb1kl7i0C},
	keywords = {Social Science / Sociology / General},
}

@book{seiler_republic_2008,
	address = {Chicago, UNITED STATES},
	title = {Republic of {Drivers}: {A} {Cultural} {History} of {Automobility} in {America}},
